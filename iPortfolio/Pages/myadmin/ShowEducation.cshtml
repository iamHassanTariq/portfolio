@page
@model iPortfolio.Pages.myadmin.ShowEducationModel
@{
}
div class="container">
<div class="row justify-content-center">
    <div class="col-md-12">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Degree</th>
                    <a href="c:\users\hassan\source\repos\iportfolio\iportfolio\pages\myadmin\showeducation.cshtml.cs">c:\users\hassan\source\repos\iportfolio\iportfolio\pages\myadmin\showeducation.cshtml.cs</a>
                    <a href="c:\users\hassan\source\repos\iportfolio\iportfolio\pages\myadmin\showeducation.cshtml">c:\users\hassan\source\repos\iportfolio\iportfolio\pages\myadmin\showeducation.cshtml</a>
                    <th scope="col">StartYear</th>
                    <th scope="col">EndYear</th>
                    <th scope="col">InstituteName</th>
                    <th scope="col">Description</th>
                    <th scope="col" colspan="2">Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.education)
                {
                    <tr>
                    <th scope="row">@item.Id</th>
                    <td>@item.Degree</td>
                    <td>@item.StartYear</td>
                    <td>@item.EndYear</td>
                    <td>@item.InstituteName</td>
                    <td>@item.Description</td>
                    <td>
                        <a class="btn btn-primary"asp-page="UpdateEducation" asp-route="@item.Id">Update</a>
                    </td>
                    <td>
                        <a class="btn btn-danger"asp-page-handler="Delete"asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
                }

            </tbody>
        </table>
    </div>
</div>

